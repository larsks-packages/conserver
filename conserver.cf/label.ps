% The relative co-ords of the various RJ11 jacks
/coords
[
	% j# x   y		jack number, +x, +y
        [ 13 0.5 1.25 ]
        [ 11 1.0 1.25 ]
        [  8 1.5 1.25 ]
        [ 14 0.5 0.625 ]
        [ 10 1.0 0.625 ]
        [  9 1.5 0.625 ]
        [ 15 0.5 0 ]
        [ 12 1.0 0 ]
        [  7 2.1875 1.25 ]
        [  5 2.6875 1.25 ]
        [  3 3.1875 1.25 ]
        [  6 2.1875 0.625 ]
        [  4 2.6875 0.625 ]
        [  0 3.1875 0.625 ]
        [  2 2.6875 0 ]
        [  1 3.1875 0 ]
